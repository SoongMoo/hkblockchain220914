<!DOCTYPE html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  <title>Document</title>
  <script>
	var num = 10; // global
	document.write(num);
	var num = "<br />이숭무<br />";
	document.write(num);
	num = 10.5
	document.write(num);
	var firstNum = "<br />숭무<br />"
	document.write(firstNum + "<br />");
	let nextNum = 10;
	document.write(nextNum + "<br />");
	// 리터널 자료 : 정수, 실수 , 문자:'이' , 문자열:"", 부울
	let result = true;
	document.write(result + "<br />");
	result = false;
	document.write(result + "<br />");
	result = null;
	document.write(result + "<br />");
	result = '이숭무';
	document.write(result + "<br />");
	// 문자, 문자열 : '',""
	result = "i'm a boby";
	document.write(result + "<br />");
	result = 'i am "a boby"';
	document.write(result + "<br />");
	// let result = "aaa";
	document.write(result + "<br />");
	var firstNum = 10;
	var secondNum = 20;
	document.write(firstNum + secondNum + "<br />");
	document.write(firstNum + "<br />" + secondNum );
  </script>
 </head>
 <body>
  
 </body>
</html>
