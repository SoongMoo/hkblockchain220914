<!DOCTYPE html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  <title>Document</title>
  <script>
	var area = "02";
	switch (area)
	{
	case "02":
		areaNum = "서울"; break;
	case "031":
		areaNum =  "경기";break;
	case "051":
		areaNum = "부산";break;
	default:
		areaNum = "다른지역입니다.";
	}
	document.write(areaNum + "<br />");
	i = 1;
	num = 0;
	while (i <= 10)
	{
		num += i;
		i += 1;
	}
	document.write(num +"<br />");

	var dan = Number(prompt("원하는 단을 입력하세요.?", 2));
	var gop = 1;
	while (gop <= 9)
	{
		document.write(dan + " * " + gop + " = " + dan*gop +"<br />" );
		gop += 1;
	}
	var dan = Number(prompt("원하는 단을 입력하세요.?", 2));
	var gop = 0;
	while(gop <= 9){
		gop += 1;
		if(gop % 2 == 0){
			document.write(dan + " * " + gop + " = " + dan*gop +"<br />" );
		}
	}
	// 홀수 출력
	var dan = Number(prompt("원하는 단을 입력하세요.?", 2));
	var gop = 0;
	while(gop <= 9){
		gop += 1;
		if(gop % 2 == 0){
			continue;
		}
		document.write(dan + " * " + gop + " = " + dan*gop +"<br />" );
	}
	
	// 홀수 출력
	var dan = Number(prompt("원하는 단을 입력하세요.?", 2));
	var gop = 0;
	while(true){
		gop += 1;
		if(gop >= 10) break;
		if(gop % 2 == 0){
			continue;
		}
		document.write(dan + " * " + gop + " = " + dan*gop +"<br />" );
	}
  </script>
 </head>
 <body>
  <div id="name">이순신</div>
  <script>
  document.getElementById("name").innerHTML="이숭무";
  var inputText ="<input type='text' />";
  document.getElementById('name').innerHTML=inputText;
  var inputText ="<table border=1 width='600px'>";
  var i = 1;
  while(i <= 10){
	  inputText += "<tr><td>1</td><td>2</td><td>3</td></tr>";
	  i += 1;
  }
  inputText +="</table>";
  document.getElementById('name').innerHTML=inputText;
 </script>
 </body>
</html>






